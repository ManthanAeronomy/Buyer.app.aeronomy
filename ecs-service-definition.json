{
  "serviceName": "aeronomy-saf-marketplace-service",
  "cluster": "aeronomy-cluster",
  "taskDefinition": "aeronomy-saf-marketplace",
  "desiredCount": 2,
  "launchType": "FARGATE",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-xxxxxxxxx",
        "subnet-yyyyyyyyy"
      ],
      "securityGroups": [
        "sg-xxxxxxxxx"
      ],
      "assignPublicIp": "ENABLED"
    }
  },
  "loadBalancer": {
    "targetGroupArn": "arn:aws:elasticloadbalancing:REGION:ACCOUNT_ID:targetgroup/aeronomy-tg/xxxxxxxxx",
    "containerName": "aeronomy-saf-marketplace",
    "containerPort": 3004
  },
  "serviceRegistries": [],
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "healthCheckGracePeriodSeconds": 60,
  "enableExecuteCommand": true
}

